    <ProductOverview ID="_Fl5lozkQEeC6voFW1_Te6Q" Name="Product Overview"/>
    <MajorFeatures ID="_Fl5lpDkQEeC6voFW1_Te6Q" Name="Major Features"/>
    <Scope ID="_Fl5lpTkQEeC6voFW1_Te6Q" Name="Scope"/>
  </Vision>
</edu.isistan.dal.ucs:UCSProject>
