<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Sad Analyzer Editor"
            extensions="sada"
            icon="icons/sample.gif"
            contributorClass="edu.isistan.sadanalyzer.editor.SadAnalyzerEditorContributor"
            class="edu.isistan.sadanalyzer.editor.SadAnalyzerEditor"
            id="edu.isistan.sadanalyzer.editor.SadAnalyzerEditor">
