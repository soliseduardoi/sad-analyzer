TYPESYSTEM edu.isistan.uima.unified.typesystems.SADANALYZERTypeSystem;

STRINGLIST TokenList = {"preserv", "timeout", "lost", "abnormal"}; 

//Simple Query - Reliability-related Keywords
Sentence{ -> CREATE(CrosscuttingConcern, "name" = "Reliability", "kind" = "Simple/Direct","sentence"=Sentence)} <- {
    (
        Token.stem == "preserv" |
        Token.lemma == "timeout" |
        Token.lemma == "lost" |
        Token.lemma == "abnormal"
    );
};