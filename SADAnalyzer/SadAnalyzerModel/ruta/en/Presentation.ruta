TYPESYSTEM edu.isistan.uima.unified.typesystems.SADANALYZERTypeSystem;

STRINGLIST TokenList = {"gui", "interface", "format", "present"}; 

//Simple Query - Presentation-related Keywords
Sentence{ -> CREATE(CrosscuttingConcern, "name" = "Presentation", "kind" = "Simple/Direct","sentence"=Sentence)} <- {
    (
        Token.lemma == "gui" |
        Token.lemma == "interface" |
        Token.lemma == "format" |
        Token.lemma == "present"
    );
};